<link rel="stylesheet" type="text/css" href="main.css">
<div class="container">
<div class="header">
<h2>WeedWiki</h2>
<h2><%= link_to "Create a new article", articles_new_path %></h2>
</div>
<div class="articles">
<%= @articles.each do |article|%>
<div class="article">
<h2><%= link_to article.title, article_path(article) %></h2>
<h6><%= link_to "delete", article_path(article), method: :delete %></h6>
</div>
<%end%>
</div>
</div>

